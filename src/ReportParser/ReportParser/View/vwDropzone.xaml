<UserControl x:Class="ReportParser.View.vwDropzone"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ReportParser.View"
             mc:Ignorable="d" 
             d:DesignHeight="155" d:DesignWidth="190">
    <Grid>
        <Border x:Name="pdfDropzone" Style="{StaticResource DropzoneB}" AllowDrop="True" Drop="pdfDropzone_Drop" MouseLeftButtonDown="pdfDropzone_MouseLeftButtonDown">
            <Border Style="{StaticResource DropzoneInnerB}">
                <Border Style="{StaticResource DropzoneInnerB2}">

                </Border>
            </Border>
        </Border>
    </Grid>
</UserControl>
